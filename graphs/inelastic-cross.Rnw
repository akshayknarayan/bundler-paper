\begin{figure}
    \centering
    <<robust:cr-inelastic,fig.showtext=TRUE,cache=TRUE,echo=FALSE,warning=FALSE,fig.width='\\columnwidth',fig.height=3>>=
library(ggplot2)
library(showtext)
font_add_google("Lato")
showtext_auto()
df <- read.csv("cr-inelastic.data", sep=" ")
df$Duration <- df$Duration.usec. / 1e6
df$ofct <- (df$Size / 12e6) + 0.05
df$Slowdown <- df$Duration / df$ofct

df <- df[df$Alg != "copa_sfq",]

ggplot(df, aes(x=Cross, y=Slowdown, fill=Alg)) + 
    geom_boxplot(outlier.shape=NA) + 
    coord_cartesian(ylim=c(1,5)) +
    ylab("Slowdown") +
    xlab(NULL) +
    scale_fill_brewer(type="qual", palette=1,
        limit=c("nimcopa_sfq", "nimbus_sfq", "bbr_sfq", "nobundler_fifo"),
        labels=c("nimcopa_sfq" = "Bundler (Copa) ", "nimbus_sfq" = "Bundler (Nimbus) ", "bbr_sfq" = "Bundler (BBR) ", "nobundler_fifo" = "Baseline"),
    ) +
    theme_minimal() +
    theme(
        legend.position="top",
        legend.title=element_blank(),
        legend.text=element_text(size=16, family="Lato", face="plain"),
        text = element_text(size=18, family="Lato", face="bold"),
    )
@
    \caption{Inelastic cross traffic.}
    \label{fig:robust:cr-inelastic}
\end{figure}
